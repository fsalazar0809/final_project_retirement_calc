<h1>Sign up</h1>

<form action="/insert_personal_information" method="post">

  <div>
    <label for="email_box">Email</label>
    <input id="email_box" name="query_email" placeholder="Choose a email..." type="text">
  </div>
                 
  <div>
    <label for="current_age_box">Current age</label>
      
    <input type="text" id="current_age_box" name="query_current_age">
  </div>
         
  <div>
    <label for="retirement_age_box">Retirement age</label>
      
    <input type="text" id="retirement_age_box" name="query_retirement_age">
  </div>
         
  <div>
    <label for="retirement_savings_box">Retirement savings</label>
      
    <input type="text" id="retirement_savings_box" name="query_retirement_savings">
  </div>
         
  <div>
    <label for="return_on_savings_box">Return on savings</label>
      
    <input type="text" id="return_on_savings_box" name="query_return_on_savings">
  </div>
         
  <div>
    <label for="life_expectancy_box">Life expectancy</label>
      
    <input type="text" id="life_expectancy_box" name="query_life_expectancy">
  </div>
         
  <div>
    <label for="monthly_contribution_box">Monthly contribution</label>
      
    <input type="text" id="monthly_contribution_box" name="query_monthly_contribution">
  </div>
         
  <div>
    <label for="user_id_box">User</label>
      
    <input type="text" id="user_id_box" name="query_user_id">
  </div>
         
  <div>
    <label for="budget_id_box">Budget</label>
      
    <input type="text" id="budget_id_box" name="query_budget_id">
  </div>
    
  <div>
    <label for="password_box">Password</label>
    <input id="password_box" name="query_password" placeholder="Choose a password..." type="password">
  </div>

  <div>
    <label for="password_confirmation_box">Password Confirmation</label>
    <input id="password_confirmation_box" name="query_password_confirmation" placeholder="Confirm your password..." type="password">
  </div>

  <button>
    Sign up
  </button>
</form>

<hr>

<p>
  Or, <a href="/personal_information_sign_in">Sign in</a> instead.
</p>
